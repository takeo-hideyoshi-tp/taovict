{
  "name": "victory-pie",
  "version": "16.1.0",
  "description": "D3 pie & donut chart component for React",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/victory-pie.git"
  },
  "author": "Angela Nicholas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/victory-pie/issues"
  },
  "homepage": "https://github.com/FormidableLabs/victory-pie",
  "engines": {
    "npm": ">=2.0.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "preversion": "npm run check",
    "version": "npm run clean && npm run build",
    "postversion": "publishr postversion -V",
    "postpublish": "publishr postpublish -V",
    "test": "npm run test-frontend",
    "start": "npm run dev",
    "babel-es": "cross-env BABEL_ENV=es babel src --out-dir es --copy-files",
    "babel-lib": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --copy-files",
    "benchmark": "npm run check-perf",
    "build-dist-dev": "webpack --bail --config ../../config/webpack/webpack.config.dev.js --colors",
    "build-dist-min": "webpack --bail --config ../../config/webpack/webpack.config.js --colors",
    "build-dist": "npm run clean-dist && concurrently --buffer 'npm run build-dist-min' 'npm run build-dist-dev'",
    "build-es": "npm run clean-es && npm run babel-es",
    "build-lib": "concurrently --buffer 'npm run clean-lib' 'npm run babel-lib'",
    "build-libs": "npm run build-lib && npm run build-es",
    "build": "concurrently --buffer 'npm run build-libs' 'npm run build-dist'",
    "check-ci": "npm run lint && npm run test-ci",
    "check-cov": "npm run lint && npm run test-cov",
    "check-dev": "npm run lint && npm run test-dev",
    "check-perf": "npm run lint-perf && npm run test-perf",
    "check": "npm run lint && npm run test",
    "chromatic-test": "chromatic test --storybook-addon --exit-zero-on-changes",
    "chromatic": "chromatic test --storybook-addon",
    "clean-dist": "rimraf dist",
    "clean-es": "rimraf es",
    "clean-lib": "rimraf lib",
    "clean": "concurrently 'npm run clean-lib' 'npm run clean-dist' 'npm run clean-es'",
    "dev": "concurrently 'npm run server-dev' 'npm run server-test'",
    "hot": "concurrently 'npm run server-hot' 'npm run server-test'",
    "lint-demo": "eslint --color demo",
    "lint-perf": "eslint --color perf",
    "lint-source": "eslint --color src",
    "lint-stories": "eslint --color stories",
    "lint-test": "eslint --color test",
    "lint": "concurrently 'npm run lint-source' 'npm run lint-demo' 'npm run lint-test' 'npm run lint-stories'",
    "server-dev": "webpack-dev-server --config ../../config/webpack/demo/webpack.config.dev.js --colors --content-base demo",
    "server-hot": "webpack-dev-server --config ../../config/webpack/demo/webpack.config.hot.js --colors --inline --hot --content-base demo",
    "server-test": "webpack-dev-server --config ../../config/webpack/webpack.config.test.js --colors",
    "storybook": "start-storybook -p 6006",
    "test-ci": "npm run test-frontend-ci",
    "test-cov": "npm run test-frontend-cov",
    "test-dev": "npm run test-frontend-dev",
    "test-frontend-ci": "karma start --browsers PhantomJS,Firefox ./config/karma/karma.conf.coverage.js",
    "test-frontend-cov": "karma start ../../config/karma/karma.conf.coverage.js",
    "test-frontend-dev": "karma start ../../config/karma/karma.conf.dev.js",
    "test-frontend-perf": "karma start ../../config/karma/karma.conf.perf.js",
    "test-frontend": "karma start ../../config/karma/karma.conf.js",
    "test-perf": "npm run test-frontend-perf",
    "version-dry-run": "publishr dry-run -V"
  },
  "dependencies": {
    "d3-shape": "^1.0.0",
    "lodash": "^4.17.5",
    "victory-core": "^24.0.0"
  },
  "publishr": {
    "files": {
      ".npmignore": ".npmignore.publishr"
    },
    "scripts": {
      "postinstall": ""
    }
  },
  "sideEffects": false
}
