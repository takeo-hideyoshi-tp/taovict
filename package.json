{
  "name": "victory",
  "version": "0.27.2",
  "description": "Data viz for React",
  "keywords": [
    "data visualization",
    "React",
    "d3",
    "charting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/formidablelabs/victory.git"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "author": "Formidable",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/formidablelabs/victory/issues"
  },
  "homepage": "https://github.com/formidablelabs/open-source/victory",
  "scripts": {
    "postinstall": "lerna exec --parallel yarn run build-libs",
    "preversion": "yarn run check",
    "version": "yarn run clean && yarn run build",
    "postversion": "publishr postversion -V",
    "postpublish": "publishr postpublish -V",
    "test": "yarn run test-frontend",
    "start": "yarn run dev",
    "babel-es": "cross-env BABEL_ENV=es babel src --out-dir es --copy-files",
    "babel-lib": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --copy-files",
    "benchmark": "yarn run check-perf",
    "build-dist-dev": "webpack --bail --config ./config/webpack/webpack.config.dev.js --colors",
    "build-dist-min": "webpack --bail --config ./config/webpack/webpack.config.js --colors",
    "build-dist": "yarn run clean-dist && concurrently --buffer 'yarn run build-dist-min' 'yarn run build-dist-dev'",
    "build-es": "yarn run clean-es && yarn run babel-es",
    "build-lib": "concurrently --buffer 'yarn run clean-lib' 'yarn run babel-lib'",
    "build-libs": "yarn run build-lib && yarn run build-es",
    "build": "concurrently --buffer 'yarn run build-libs' 'yarn run build-dist'",
    "check-ci": "yarn run lint && yarn run test-ci",
    "check-cov": "yarn run lint && yarn run test-cov",
    "check-dev": "yarn run lint && yarn run test-dev",
    "check-perf": "yarn run lint-perf && yarn run test-perf",
    "check": "yarn run lint && yarn run test",
    "chromatic-test": "chromatic test --storybook-addon --exit-zero-on-changes",
    "chromatic": "chromatic test --storybook-addon",
    "clean-es": "rimraf es",
    "clean-lib": "rimraf lib",
    "clean": "concurrently 'yarn run clean-lib' 'yarn run clean-dist' 'yarn run clean-es'",
    "dev": "concurrently 'yarn run server-dev' 'yarn run server-test'",
    "hot": "concurrently 'yarn run server-hot' 'yarn run server-test'",
    "lint-demo": "eslint --color demo",
    "lint-perf": "eslint --color perf",
    "lint-source": "eslint --color packages",
    "lint-stories": "eslint --color stories",
    "lint-test": "eslint --color test",
    "lint": "concurrently 'yarn run lint-source' 'yarn run lint-demo' 'yarn run lint-test' 'yarn run lint-stories'",
    "server-dev": "webpack-dev-server --config ./config/webpack/demo/webpack.config.dev.js --colors --content-base demo",
    "server-hot": "webpack-dev-server --config ./config/webpack/demo/webpack.config.hot.js --colors --inline --hot --content-base demo",
    "server-test": "webpack-dev-server --config ./config/webpack/webpack.config.test.js --colors",
    "storybook": "start-storybook -p 6006",
    "test-ci": "yarn run test-frontend-ci",
    "test-cov": "yarn run test-frontend-cov",
    "test-dev": "yarn run test-frontend-dev",
    "test-frontend-ci": "karma start --browsers PhantomJS,Firefox ./config/karma/karma.conf.coverage.js",
    "test-frontend-cov": "karma start ./config/karma/karma.conf.coverage.js",
    "test-frontend-dev": "karma start ./config/karma/karma.conf.dev.js",
    "test-frontend-perf": "karma start ./config/karma/karma.conf.perf.js",
    "test-frontend": "karma start ./config/karma/karma.conf.js",
    "test-perf": "yarn run test-frontend-perf",
    "version-dry-run": "publishr dry-run -V"
  },
  "devDependencies": {
    "@storybook/addon-options": "^3.1.2",
    "@storybook/react": "^3.2.13",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-check-es2015-constants": "^6.22.0",
    "babel-plugin-lodash": "^3.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-computed-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-native": "^1.9.0",
    "chai": "^3.5.0",
    "concurrently": "^3.6.0",
    "core-js": "^2.5.1",
    "cross-env": "^5.2.0",
    "d3-scale": "^1.0.0",
    "d3-shape": "^1.2.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "eslint": "^2.0.0",
    "eslint-config-formidable": "^2.0.1",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^6.10.3",
    "immutable": "^3.8.2",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-benchmark": "^0.6.0",
    "karma-benchmarkjs-reporter": "^1.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.4",
    "lerna": "^2.11.0",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^5.0.0",
    "phantomjs-prebuilt": "^2.1.7",
    "prop-types": "^15.5.8",
    "publishr": "^1.0.0",
    "react": "^16.0.0",
    "react-chromatic": "^0.8.2",
    "react-dom": "^16.0.0",
    "react-hot-loader": "v2.0.0-alpha-4",
    "rimraf": "^2.4.0",
    "seedrandom": "^2.4.3",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^1.10.0",
    "webpack-stats-plugin": "^0.1.1"
  },
  "publishr": {
    "files": {
      ".npmignore": ".npmignore.publishr"
    },
    "scripts": {
      "postinstall": ""
    }
  },
  "sideEffects": false
}
